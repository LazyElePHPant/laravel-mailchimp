{
    "name": "lazyelephpant/mailchimp",
    "description": "MailChimp API wrapper to manage members of a specific list, including subscribed, unsubscribed, and bounced members.",
    "type": "library",
    "authors": [
        {
            "name": "LazyElePHPant",
            "email": "kevin@kevinpimentel.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.3"
    },
    "autoload": {
      "psr-4": {
        "LazyElePHPant\\MailChimp\\":"src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "LazyElePHPant\\MailChimp\\MailChimpServiceProvider"
        ],
        "aliases": {}
      }
    }
}
